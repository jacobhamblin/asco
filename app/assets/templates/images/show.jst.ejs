<div class="header">
  <center>
    <a href="#users/<%= image.owner().id %>" class="image-show-title">
    <%= image.owner().escape('username') ? image.owner().escape('username') : image.owner().escape('email') %>
     &#8212 grid </a>
  </center>
</div>

<div class="image-show clearfix">
  <% if (prevImage) { %>
    <a href="#images/<%= prevImage.id %>" class="prev-image arrows"><img src="https://s3-us-west-1.amazonaws.com/asco-jkh/layout/Arrow.svg"></a>
  <% } %>
  <img src="<%= image.escape('url')%>" class="image">
  <% if (nextImage) { %>
    <a href="#images/<%= nextImage.id %>" class="next-image arrows"><img src="https://s3-us-west-1.amazonaws.com/asco-jkh/layout/Arrow.svg"></a>
  <% } %>
</div>
